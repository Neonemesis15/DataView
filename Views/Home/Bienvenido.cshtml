<link href="@Url.Content("~/Content/bootstrap/css/bootstrap.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("~/Scripts/jquery-1.7.1.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery-ui-1.8.11.min.js")" type="text/javascript"></script>
	<style>
	.cls_home_todo{
		position:absolute;
		width:100%;
		height:90%;
	}
	
		.cls_home_content{
			
			width:100%;
			height:50%;
		}
		.cls_home_separador{
			display:inline-block;
			width:18%;
			height:100%;
			margin:20px;
		
		}
		.cls_home_separador_onda{
			display:inline-block;
			width:4%;
			height:100%;
			
		
		}
		.cls_home_img{
			background-color:white;
			width:90%;
			height:95%;
			border:solid 1px #BDBDBD;
			border-radius:20px 0px 20px 0px;
			padding:10px;
			
		}
		.cls_home_img:hover{
			-webkit-box-shadow: 2px 2px 5px #424242;
		  -moz-box-shadow: 2px 2px 5px #424242;
		  filter: shadow(color=#424242, direction=135, strength=2);
		}
		.cls_home_img_02{
			border-radius:20px 0px 20px 0px;
			width:100%;
			height:100%;
			border:solid 1px #BDBDBD;
			background-color:white;
            background-size:80%;
		}
		.cls_home_sombra{
			-webkit-box-shadow: 2px 2px 5px #999;
		  -moz-box-shadow: 2px 2px 5px #999;
		  filter: shadow(color=#999999, direction=135, strength=2);
		
		/* navegadores sin soporte */
		background: black url(images/cabecera_degradado.jpg) repeat-x;
		/* Safari 4-5, Chrome 1-9 */
		background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(white), to(#D8D8D8));
		/* Safari 5.1+, Chrome 10+ */
		background: -webkit-linear-gradient(top, #D8D8D8, white);
		/* Firefox 3.6+ */
		background: -moz-linear-gradient(top, #D8D8D8, white);
		/* Opera 11.10+ */
		background: -o-linear-background(top, #D8D8D8, white);
		/* IE 10 */
		background: -ms-linear-background(top, #D8D8D8, white);
		/* estándar */
		background: linear-background(top, #D8D8D8, white);
		
		}
		.cls_home_som_select{
			-webkit-box-shadow: 3px 3px 5px #A5DF00;
		  -moz-box-shadow: 3px 3px 5px #A5DF00;
		  filter: shadow(color=#A5DF00, direction=135, strength=2);
		
		}
		.cls_pie{
			margin-top:3%;
			width:50%;
			height:41%;
			
		}
	.cls_home_img_onda_01{
		display:online-block;
		position:relative;
		width:50%;
		top:-50%;
		right:25%;
	}
	.cls_home_img_onda_02{
		width:50%;
		display:online-block;
		position:relative;
		top:-50%;
	}
	.cls_hover:hover{
		color:#BDBDBD;
		cursor:pointer;
	}
	.cls_hover{
		color:white;
	}
	</style>
	<script>
	    $(document).ready(function () {
	        $(".cls_home_img").hide();
	        $(".cls_home_img_onda_01").hide();
	        $(".cls_home_img_onda_02").hide();
	        $("#dv_01").show("blind", 1000, function () {

	            $("#dv_02").show("blind", 1000, function () {
	                $("#dv_03").show("blind", 1000, function () {
	                    $("#dv_04").show("blind", 1000);
	                });
	            });
	        });
	        funct_onda();
	    });
	    function funct_onda() {
	        setInterval(function () {
	            if ($('.cls_home_img_onda_01').is(':hidden')) {
	                $(".cls_home_img_onda_01").show();
	                $(".cls_home_img_onda_02").hide();
	            } else {
	                $(".cls_home_img_onda_01").hide();
	                $(".cls_home_img_onda_02").show();
	            }
	        }, 500);

	    }
	</script>
</head>
<body>
<br>
<center>
	<div class="cls_home_todo">
		<div class="cls_home_content">
			<div class="cls_home_separador">
				<div class="cls_home_img cls_home_sombra"  id="dv_01" >
					<div class="cls_home_img_02" style="background-image:url('../../images/celular02.png');background-repeat:no-repeat;background-position:center;background-size:contain;">
						
					</div>					
				</div>
			</div>
			<div class="cls_home_separador_onda">
				<img src="../../images/arrow-30-48.png" class="cls_home_img_onda_01" />
				<img src="../../images/arrow-30-48.png" class="cls_home_img_onda_02" />
			</div>
			<div class="cls_home_separador">
				<div class="cls_home_img cls_home_sombra" id="dv_02">
					<div class="cls_home_img_02" style="background-image:url('../../images/datos02.png');background-repeat:no-repeat;background-position:center;background-size:contain;" ></div>
				</div>
			</div>
			<div class="cls_home_separador">
				<div class="cls_home_img cls_home_sombra cls_home_som_select" id="dv_03">
					<div class="cls_home_img_02" style="background-image:url('../../images/accept-database-icon.png');background-repeat:no-repeat;background-position:center;background-size:contain;"></div>
				</div>
			</div>
			<div class="cls_home_separador">
				<div class="cls_home_img cls_home_sombra" id="dv_04">
					<div class="cls_home_img_02" style="background-image:url('../../images/grafico.jpg');background-repeat:no-repeat;background-position:center;background-size:contain;"></div>
				</div>
			</div>
		</div>
		<div class="cls_pie">
		<img src="../../images/cabecera02.png" class="cls_home_sombra" style="margin-top:10px;border-radius:5px;"/>
		<br>
		<br>
		<p class="muted"> Modulo Data Mercaderista </p>
		<p class="muted"> Contactenos | Términos de uso Lucky SAC © 2014 . Equipo de Tecnologia. </p>
		<b class="text-success cls_hover">CPHGD</b>
		</div>
	</div>
</center>
</body>